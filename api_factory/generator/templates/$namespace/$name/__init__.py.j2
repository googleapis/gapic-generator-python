{% extends '_base.py.j2' %}

{% block content %}
{% for service in api.services.values() -%}
from ..{{ api.versioned_module_name }}.{{ service.name|snake_case }} import {{ service.name }}
{% endfor %}
__all__ = (
    {%- for service in api.services.values() %}
    '{{ service.name }}',
    {%- endfor %}
)
{% endblock %}

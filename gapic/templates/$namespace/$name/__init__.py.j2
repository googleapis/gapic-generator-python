{% extends '_base.py.j2' %}

{% block content %}
from . import {{ api.naming.version }}

# Import each service from {{ api.naming.version }} into the unversioned namespace.
{% for service in api.services.values() -%}
from .{{ api.naming.version }}.services.{{ service.name|snake_case }} import {{ service.name }}
{% endfor %}
__all__ = (
    '{{ api.naming.version }}',
    {%- for service in api.services.values() %}
    '{{ service.name }}',
    {%- endfor %}
)
{% endblock %}
